{
    "name": "innovagile/hack-summit",
    "description": "A charity app based on Slim Framework skeleton application - slim/slim-skeleton",
    "keywords": ["project"],
    "homepage": "http://169.54.146.72/",
    "autoload": {
        "psr-0": { "CharityApp": "src/" }
    },
    "require": {
        "incenteev/composer-parameter-handler": "~2.0",
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    }
}
